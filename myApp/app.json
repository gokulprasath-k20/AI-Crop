{
  "expo": {
    "name": "SIH Crop Advisor",
    "slug": "sih-crop-advisor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sih-crop-advisor",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "AI-powered crop recommendation app for Jharkhand farmers - SIH 2025",
    "keywords": ["agriculture", "farming", "crop", "recommendation", "AI", "Jharkhand", "SIH"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sih2025.cropadvisor"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F4F1E8",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "package": "com.sih2025.cropadvisor",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#F4F1E8",
          "dark": {
            "backgroundColor": "#2E7D32"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
